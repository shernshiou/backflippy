{
  "manifest_version": 2,

  "name": "Backflippy",
  "description": "FSecure Hackathon",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "16": "images/stack16.png",
    "48": "images/stack48.png",
    "128": "images/stack128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "images/stack19.png",
      "38": "images/stack38.png"
    },
    "default_title": "Backflip"
  },
  "permissions" : ["contextMenus"]
}
